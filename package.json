{
	"name": "ftc-for-vs-code",
	"displayName": "FTC for VS Code",
	"description": "An extension for accessing FTC snippets, debugger, and cmdline tools from a button",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ftc-for-vs-code.helloWorld",
		"onCommand:ftc-for-vs-code.runApp"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ftc-for-vs-code.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "ftc-for-vs-code.runApp",
				"title": "Build and Run FTC App",
				"icon": {
					"dark": "resources/play-icon.svg",
					"light": "resources/play-icon.svg"
				}
			}
		],
		"snippets": [
			{
				"language": "java",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"list-github-dir-content": "^3.0.0",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
